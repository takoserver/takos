{
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "solid-js",
    "types": ["vite/client"],
    "lib": ["dom", "es2024", "deno.ns"]
  },
  "tasks": {
    "dev": "deno run -A --node-modules-dir --unstable-detect-cjs npm:vite dev",
    "build": "deno run -A --node-modules-dir --unstable-detect-cjs npm:vite build"
  },
  "imports": {
    "@motionone/solid": "npm:@motionone/solid@^10.16.4",
    "@tailwindcss/vite": "npm:@tailwindcss/vite@^4.1.11",
    "solid-js": "npm:solid-js@^1.9.6",
    "jotai": "npm:jotai@^2.12.4",
    "solid-jotai": "npm:solid-jotai@^0.3.1",
    "markdown-it": "npm:markdown-it@^14.1.0",
    "tailwindcss": "npm:tailwindcss@^4.1.11",
    "vite": "npm:vite@^5.4.19",
    "vite-plugin-solid": "npm:vite-plugin-solid@^2.11.6"
  },
  "nodeModulesDir": "auto"
}
