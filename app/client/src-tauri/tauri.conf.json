{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "takos",
  "version": "0.1.0",  "identifier": "com.tauri.dev",
  "build": {
    "beforeDevCommand": "deno task dev",
    "beforeBuildCommand": "deno task build",
    "devUrl": "http://localhost:1420"
  },
  "app": {
    "windows": [
      {
        "title": "takos",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "plugins": {
    "store": {
      "default": true
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
